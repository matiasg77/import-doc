{
	"name": "import-doc-link",
	"displayName": "import-doc-link",
	"description": "Link to the module documentation",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:importDocLink.toggle"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Import Doc configuration",
			"properties": {
				"importDocLink.typescriptExtensions": {
					"type": "array",
					"default": [
						"\\.tsx?$"
					],
					"description": "File extensions to be parsed by the Typescript parser"
				},
				"importDocLink.javascriptExtensions": {
					"type": "array",
					"default": [
						"\\.jsx?$"
					],
					"description": "File extensions to be parsed by the Javascript parser"
				},
				"importDocLink.vueExtensions": {
					"type": "array",
					"default": [
						"\\.vue$"
					],
					"description": "File extensions to be parsed by the Vue parser"
				},
				"importDocLink.svelteExtensions": {
					"type": "array",
					"default": [
						"\\.svelte$"
					],
					"description": "File extensions to be parsed by the Svelte parser"
				},
				"importDocLink.bundleSizeDecoration": {
					"type": "string",
					"default": "both",
					"enum": [
						"both",
						"minified",
						"gzipped"
					],
					"description": "Which bundle size to display"
				},
				"importDocLink.showCalculatingDecoration": {
					"type": "boolean",
					"default": true,
					"description": "Display the 'calculating' decoration when starting to calculate the size"
				},
				"importDocLink.debug": {
					"type": "boolean",
					"default": false,
					"description": "Enable debug logging"
				},
				"importDocLink.timeout": {
					"type": "number",
					"default": 10000,
					"description": "Size calculation timeout in milliseconds (requires restart)"
				}
			}
		},
		"commands": [
			{
				"command": "importDocLink.toggle",
				"title": "Toggle Import Doc"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"@babel/parser": "^7.12.7",
		"@babel/traverse": "^7.12.9",
		"@babel/types": "^7.12.7",
		"cheerio": "^1.0.0-rc.9",
		"memory-cache": "^0.2.0",
		"pkg-dir": "^5.0.0"
	}
}

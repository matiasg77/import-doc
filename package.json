		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"@babel/parser": "^7.12.7",
		"@babel/traverse": "^7.12.9",
		"@babel/types": "^7.12.7",
		"cheerio": "^1.0.0-rc.9"
	}
